all: png pdf gif

png:
	mkdir -p pngs/large pngs/small
	for i in $(FILE);do \
		rsvg-convert svgs/large/$$i.svg > pngs/large/$$i.png; \
		rsvg-convert svgs/small/$$i.svg > pngs/small/$$i.png; \
	done
pdf:
	mkdir -p pdfs/large pdfs/small
	for i in $(FILE);do \
		rsvg-convert -f pdf svgs/large/$$i.svg > pdfs/large/$$i.pdf; \
		rsvg-convert -f pdf svgs/small/$$i.svg > pdfs/small/$$i.pdf; \
	done
gif:
	mkdir -p gifs/large gifs/small
	for i in $(FILE);do \
		convert pngs/large/$$i.png gifs/large/$$i.gif; \
		convert pngs/small/$$i.png gifs/small/$$i.gif; \
	done

clean:
	rm -fr pngs
	rm -fr pdfs
	rm -fr gifs